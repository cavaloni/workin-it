{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "A React template",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev-build": "run-p start webpack:dev",
    "webpack:dev": "webpack --watch --progress --colors",
    "prod": "run-p start build-prod",
    "heroku-postbuild": "npm run build-prod",
    "dev": "webpack-dev-server --inline --hot",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build-prod": "webpack -p --progress --colors --config webpack.config.js",
    "lint": "eslint --fix js/"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "author": "Joe Turner <joe@oampo.co.uk>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.1",
    "chokidar-cli": "^1.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "expect": "^1.20.2",
    "http-server": "^0.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.0",
    "npm-run-all": "^3.0.0",
    "react-addons-test-utils": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-css-modules": "^2.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.1",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.0",
    "material-ui-number-input": "^5.0.17",
    "react": "^15.3.1",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.3.1",
    "react-google-charts": "^1.5.3",
    "react-material-ui-number-input": "^1.0.4",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-burger-menu": "^0.1.0",
    "redux-thunk": "^2.1.0",
    "rxjs": "^5.1.0",
    "style-loader": "^0.13.1",
    "uglify-js": "github:mishoo/UglifyJS2#harmony",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1"
  }
}
